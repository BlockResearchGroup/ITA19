                    <item name="CodeInput" type_name="gh_string" type_code="10">import sys
import os
sys.path.append(os.path.abspath(os.path.join(HERE, "..")))

from compas.datastructures import Mesh
from compas.geometry import Transformation, Translation, Box, Frame

from assembly import Assembly
from assembly import Element

# number of bricks in even courses
number_of_even_bricks = 5

# number of courses
number_of_courses = 7

# brick dimensions
w, l, h = dimensions

# brick geometry
box = Box(Frame.worldXY(), w, l, h)
T = Translation([-w/2., -l/2, 0])
box.transform(T)
brick = Element.from_mesh(Mesh.from_shape(box))
brick.transform(Translation(list(origin)))

# halfbrick geometry

box = Box(Frame.worldXY(), w * 0.5, l, h)
T = Translation([-w/2., -l/2, 0])
box.transform(T)
halfbrick = Element.from_mesh(Mesh.from_shape(box))
halfbrick.transform(Translation(list(origin)))

# empty assembly

assembly = Assembly()

# add bricks in a staggered pattern
num = 0

course_length = w * number_of_even_bricks + (number_of_even_bricks - 1) * gap

for i in range(number_of_courses):
    
    dy = i * h

    if i % 2 == 0:
        step = course_length/number_of_even_bricks
        # in the even rows
        # add (number_of_even_bricks) full bricks

        for j in range(number_of_even_bricks):
            block = brick.copy()
            block.transform(Translation([j * step, 0, dy]))
            assembly.add_element(block)
            num+=1
    else:
        gap_t = (number_of_even_bricks - 1)/float(number_of_even_bricks) * gap
        step = w + gap_t
        # in the uneven rows
        # add a half brick
        # add (number_of_even_bricks - 1) full bricks
        # add a half brick

        block = halfbrick.copy()
        block.transform(Translation([0, 0, dy]))
        assembly.add_element(block)

        for j in range(number_of_even_bricks - 1):
            block = brick.copy()
            block.transform(Translation([(0.5 + j) * step, 0, dy]))
            assembly.add_element(block)

        block = halfbrick.copy()
        block.transform(Translation([(0.5 + j + 1) *  step, 0, dy]))
        assembly.add_element(block)
        num+=1

print num</item>
