                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Vector
from compas.geometry import Point

from compas_ghpython.utilities import draw_pipes
from System.Drawing import Color

red = Color.FromArgb(255, 0, 0)
green = Color.FromArgb(0, 255, 0)
blue = Color.FromArgb(0, 0, 255)

frame_colors = [red, green, blue]

# TODO: move to frame artist?
def pipes_from_frame(frame, size, radius):
    pipes = []
    for axis in [frame.xaxis, frame.yaxis, frame.zaxis]:
        pipe = {}
        pipe['points'] = [list(frame.point), list(Point(*frame.point) + Vector(*axis) * size)]
        pipe['radius'] = radius
        pipes.append(pipe)
    return pipes


pipe_size = 0.1 * robot.scale_factor
pipe_radius = 0.001 * robot.scale_factor
    

pipes = []
frames_color = []
for frame in frames:
    if not frame:
        continue
    pipes += pipes_from_frame(frame, pipe_size, pipe_radius)
    frames_color += frame_colors
    
G = draw_pipes(pipes, cap=1)
S = frames_color</item>
