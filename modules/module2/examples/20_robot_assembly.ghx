                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas_ghpython.utilities import draw_frame
from compas_fab.robots import Configuration

if robot and configuration:

    robot.artist.update(configuration, robot.get_configurable_joint_names(), visual=show_visual, collision=show_collision)

    print("Configuration:")
    print(configuration.values)
    
    axes = robot.transformed_axes(configuration)

    if show_frames:
        frames = robot.transformed_frames(configuration)

    ee_frame_RCF = robot.forward_kinematics(configuration, backend='model')
    ee_frame = robot.to_world_coords(ee_frame_RCF)
    ee_plane = draw_frame(ee_frame)

    print("End-effector frame:")
    print(ee_frame)
    
    if show_visual:
        visual = robot.draw_visual()
        print("visual", visual)
    
    
    if robot.attached_tool:
        ee_frame = robot.to_tool_frame(ee_frame)

    if show_collision:
        collision = robot.draw_collision()

    base_frame = robot.get_base_frame()
    base_plane = draw_frame(base_frame)

if not show_end_effector_frame:
    ee_frame = None

if not show_base_frame:
    base_frame = None


